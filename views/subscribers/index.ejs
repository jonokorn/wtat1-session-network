<h2>Subscribers Table</h2>
<table>
    <thead>
        <tr>
            <th class="text-red-400">Name</th>
            <th>Email</th>
            <th>Zip-Code</th>
           
        </tr>
    </thead>
    <tbody>
        <% subscribers.forEach(subscriber => { %>
            <tr>
                <a href="<%= `/subscribers/${subscriber._id}` %>">
                    <%= subscriber.name %>
                </a>
                <td><%= subscriber.email %></td>
                <td><%= subscriber.zipCode %></td>
                <td>
                    <a href=<%=`/subscribers/${subscriber._id}/edit` %>  >
                        Edit
                    </a>
                </td>
                <td>
                    <a href="<%= `subscribers/${subscriber._id}/delete?_method=DELETE` %>">
                    Delete
                    </a>
                </td> 
            </tr>
        <% }); %>
    </tbody>
</table>